generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  clerkID   String  @id @default(auto()) @map("_id") @db.ObjectId
  email     String  @unique
  username  String? @unique
  firstName String?
  lastName  String?
  photo     String?

  @@map("users")
}
